{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dms",
  "version": "0.1.0",
  "identifier": "com.dms.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DMS-System",
        "fullscreen": false,
        "width": 800,
        "height": 600,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../src/assets/dms-logo.jpg"
    ]
  }
}
